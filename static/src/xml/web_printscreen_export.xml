<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-extend="ListView.buttons">
        <t t-if='widget.options.import_enabled' t-jquery="button.o_list_button_add" t-operation="after">
            <button class="btn btn-sm btn-default oe_bold oe_list_button_import_pdf"
                   id="button_export_pdf" type="button"
                   href="#">PDF</button>
            <button class="btn btn-sm btn-default oe_bold oe_list_button_import_excel"
                   id="button_export_excel" type="button"
                   href="#">Excel</button>
        </t>
    </t>
</templates>